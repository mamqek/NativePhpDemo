LAB_API_PORT=8000
LAB_WEB_PORT=5173

# Optional override for local NativePHP command wrappers.
# If unset, scripts auto-detect .tools/php83 and then fallback to php in PATH.
NATIVEPHP_PHP_BIN=

# Optional override for Jump host IP.
# If unset, scripts auto-detect first non-loopback IPv4.
NATIVEPHP_HOST_IP=

# Optional guest mode toggle used by mobile app.
VITE_ALLOW_GUEST_MODE=true
